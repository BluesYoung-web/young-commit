{
  "name": "young-commit",
  "type": "module",
  "version": "2.4.3",
  "description": "一个用于规范化 git 提交的命令行工具，可以根据 git 提交记录自动生成 changelog",
  "author": "BluesYoung-web",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:BluesYoung-web/young-commit.git"
  },
  "keywords": [
    "nodejs",
    "git-commit",
    "generate changelog"
  ],
  "main": "index.mjs",
  "bin": {
    "young-commit": "index.mjs",
    "yc": "index.mjs"
  },
  "files": [
    "init.mjs",
    "index.mjs",
    "dist"
  ],
  "scripts": {
    "dev": "jiti ./src/index.ts",
    "build": "unbuild",
    "push": "npm publish --access public",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "node ./init.mjs"
  },
  "dependencies": {
    "changelogen": "^0.5.5",
    "citty": "^0.1.3",
    "execa": "^8.0.1",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2",
    "semver": "^7.5.4",
    "tasuku": "^2.0.1",
    "which": "^4.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@types/node": "^20.5.9",
    "@types/prompts": "^2.4.4",
    "@types/semver": "^7.5.1",
    "@types/which": "^3.0.0",
    "eslint": "^8.48.0",
    "jiti": "^1.19.3",
    "lint-staged": "^14.0.1",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
