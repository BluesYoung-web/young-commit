{
  "name": "young-commit",
  "version": "1.0.2",
  "description": "一个用于规范化 git 提交的命令行工具，可以根据 git 提交记录自动生成 changelog",
  "bin": {
    "young-commit": "./dist/index.mjs",
    "yc": "./dist/index.mjs"
  },
  "scripts": {
    "dev": "jiti ./src/index.ts",
    "build": "unbuild"
  },
  "keywords": [
    "nodejs",
    "git-commit",
    "generate changelog"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "BluesYoung-web",
  "repository": {
    "type": "git",
    "url": "git@github.com:BluesYoung-web/young-commit.git"
  },
  "license": "MIT",
  "dependencies": {
    "changelogen": "^0.5.5",
    "citty": "^0.1.3",
    "execa": "^8.0.1",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2",
    "semver": "^7.5.4",
    "tasuku": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.9",
    "@types/prompts": "^2.4.4",
    "@types/semver": "^7.5.1",
    "jiti": "^1.19.3",
    "rome": "^12.1.3",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0"
  }
}